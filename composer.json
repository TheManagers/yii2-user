{
    "name": "themanagers/yii2-user",
    "description": "Flexible user registration and authentication module for Yii2 with MongoDB",
    "keywords": ["yii2", "yii2-user", "user management", "dektrium", "themanagers"],
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
            {
              "name": "Fabian F.",
              "email": "lostchaos3@gmail.com"
            },
            {
              "name": "Jonas B.",
              "email": "jonas.bruechner@gmail.com"
            }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-swiftmailer": "~2.0",
        "yiisoft/yii2-authclient": "~2.0",
        "yiisoft/yii2-jui": "~2.0",
        "yiisoft/yii2-bootstrap": "~2.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "codeception/aspect-mock": "*"
    },
    "autoload": {
        "psr-4": {
            "dektrium\\user\\": ""
        }
    },
    "extra": {
        "bootstrap": "dektrium\\user\\Bootstrap",
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
